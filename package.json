{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npm run prisma:generate && concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "start": "npm run prisma:generate && node dist/server.js",
    "prisma:generate": "prisma generate --schema=src/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/database/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.5.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.10.1",
    "typescript": "^5.8.3"
  }
}
